{
    "compilerOptions":{
        "target": "ES2020",
        "noImplicitAny": false,
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "moduleResolution": "node"
    }
}
